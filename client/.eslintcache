erofficial</span></h6>\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n                        \r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Homepage;","G:\\Github repos\\GreyPaper\\client\\src\\views\\LandingPage.jsx",["44","45","46","47"],"G:\\Github repos\\GreyPaper\\client\\src\\components\\bottomRow.jsx",["48","49"],{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","severity":1,"message":"55","line":1,"column":17,"nodeType":"56","messageId":"57","endLine":1,"endColumn":26},{"ruleId":"54","severity":1,"message":"58","line":2,"column":34,"nodeType":"56","messageId":"57","endLine":2,"endColumn":40},{"ruleId":"54","severity":1,"message":"59","line":13,"column":8,"nodeType":"56","messageId":"57","endLine":13,"endColumn":14},{"ruleId":"60","severity":1,"message":"61","line":132,"column":166,"nodeType":"62","endLine":132,"endColumn":198},{"ruleId":"60","severity":1,"message":"61","line":138,"column":164,"nodeType":"62","endLine":138,"endColumn":196},{"ruleId":"54","severity":1,"message":"63","line":3,"column":8,"nodeType":"56","messageId":"57","endLine":3,"endColumn":13},{"ruleId":"64","severity":1,"message":"65","line":72,"column":33,"nodeType":"66","endLine":72,"endColumn":89},{"ruleId":"64","severity":1,"message":"65","line":75,"column":33,"nodeType":"66","endLine":75,"endColumn":89},{"ruleId":"64","severity":1,"message":"65","line":78,"column":33,"nodeType":"66","endLine":78,"endColumn":89},{"ruleId":"60","severity":1,"message":"61","line":51,"column":162,"nodeType":"62","endLine":51,"endColumn":194},{"ruleId":"60","severity":1,"message":"61","line":57,"column":164,"nodeType":"62","endLine":57,"endColumn":196},"no-native-reassign",["67"],"no-negated-in-lhs",["68"],"no-unused-vars","'useEffect' is defined but never used.","Identifier"